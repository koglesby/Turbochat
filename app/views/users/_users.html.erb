<% users.each do |user| %>
  <% room_name = get_name(user, current_user)%>
  <% current_room = 
    Room.where(name: room_name).first || 
    Room.create_private_room([user, current_user], room_name) %>
  <% last_message_user = current_room&.latest_message&.user%>
  <% sender = current_user.eql?(user) ? current_user : user %>
  <%= render 'users/user',
  user: user,
  last_message: current_room&.latest_message,
  sender:,
  room: current_room
  %>
<% end %>
